<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bionime.mapper.EquipmentRecordMapper">
	<select id="insert" resultType="com.bionime.pojo.EquipmentRecord" parameterType="com.bionime.pojo.EquipmentRecord">
		insert into equipment_record (e_id,in_hospital,be_time,me_time,fe_time) 
		values(#{e_id},#{in_hospital},#{be_time},#{me_time},#{fe_time})
	</select>
</mapper>